package hardware;

public class Server {
	static private int id=0;
	private int uniqueID=0;
	private int average;
	private int max;
		
	
	public Server(){
		id++;
		this.uniqueID=id;
		this.max=-1;
		this.average=-1;
	}
	
	
	public int getMax(){
		return max;
	}
	
	public int getAverage(){
		return average;
	}
	
	public int getID(){
		return uniqueID;
	}
	
	
	
	
}
